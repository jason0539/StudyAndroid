<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jason.workdemo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".MainActivity"
                android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".touchevent.velocity.VelocityTrackerDemoActivity"></activity>
        <activity android:name=".demo.SchemeDemoActivity"></activity>
        <activity android:name=".touchevent.gesture.GestureActivity"></activity>
        <activity android:name=".animation.AnimActivity"></activity>
        <activity android:name=".animation.ScrollActivity"></activity>
        <activity android:name=".touchevent.event.TouchActivity"></activity>
        <activity android:name=".touchevent.scrollconflict.ScrollConflictActivity"></activity>
        <activity android:name=".plugin.PluginActivity"></activity>
        <activity android:name=".demo.HandlerDemoActivity"></activity>
        <activity android:name=".view.CustomViewActivity"></activity>
        <activity android:name=".demo.webview.WebviewActivity"></activity>
        <activity android:name=".demo.ListViewActivity"></activity>
        <activity android:name=".demo.span.SpanDemoActivity"></activity>
        <activity android:name=".demo.TextViewActivity"></activity>
        <activity android:name=".demo.scroller.ScrollerActivity"></activity>
    </application>

</manifest>